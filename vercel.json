{
  "builds": [
    {
      "src": "website/website/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11",
        "requirementsPath": "website/requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "website/website/wsgi.py"
    }
  ]
}
